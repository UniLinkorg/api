{
	"$schema": "https://biomejs.dev/schemas/1.9.2/schema.json",
	"formatter": {
		"enabled": true,
		"lineWidth": 80
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double",
			"semicolons": "always",
			"trailingCommas": "none"
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}